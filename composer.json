  "version": "2.2.2",
  "type": "library",
  "keywords": ["forms", "PHP", "API"],
  "homepage": "https://github.com/degami/php-forms-api",
  "license": "GNU Public License",
  "authors": [
    {
      "name": "degami@github.com",
      "homepage": "https://github.com/degami/php-forms-api"
    }
  ],
  "require": {
      "components/jqueryui": "*"
  },
  "autoload": {
    "files": ["src/php_forms_api_defines.php"],
    "psr-4": {
